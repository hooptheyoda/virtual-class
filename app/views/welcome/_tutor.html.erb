<h3>Manage classrooms:</h3>
<ul>
  <% Classroom.where("id != #{$officeid}").each do |room| %>
    <li>
      <%= room.subject %> -
      <%= link_to "Edit", edit_classroom_path(room) %> -
      <%= link_to "Schedule", classroom_path(room) %> -
      <%= link_to "Close", classroom_path(room), method: :delete %>
    </li>
  <% end %>
</ul>
<br>

<%= link_to "New Classroom", new_classroom_path %>
<%= link_to "Check Conflicts", '/check_conflicts', method: :post %>
